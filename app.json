{
  "expo": {
    "name": "todo-app-rn",
    "slug": "todo-app-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/final-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.ddsa.Tasks",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/final-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
